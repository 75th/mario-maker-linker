{
	"manifest_version": 2,
	"name": "Super Mario Maker Course ID Linkifier",
	"version": "0.1",
	"version_name": "1.0 beta 1",
	"description": "Converts all Super Mario Maker Course IDs into Bookmark links",
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"exclude_matches": ["*://supermariomaker.nintendo.com/*"],
			"js": ["mariomaker.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"128": "icon-128.png",
		"48": "icon-48.png"
	}
}
