{
	"manifest_version": 2,
	"name": "Super Mario Maker Course ID Linkifier",
	"version": "0.1",
	"version_name": "1.0 alpha 1",
	"description": "Converts all Super Mario Maker Course IDs into Web Portal links",
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"exclude_matches": ["*://supermariomaker.nintendo.com/*"],
			"js": ["mariomaker.js"],
			"run_at": "document_end"
		}
	]
}
