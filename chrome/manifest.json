{
	"manifest_version": 2,
	"name": "Super Mario Maker Web Portal links",
	"version": "0.1",
	"version_name": "1.0 alpha 1",
	"default_locale": "en",
	"description": "Converts all Super Mario Maker Course IDs into Web Portal links",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"exclude_matches": ["*://supermariomaker.nintendo.com/"],
			"js": ["mariomaker.js"],
			"run_at": "document_end"
		}
	]
}
